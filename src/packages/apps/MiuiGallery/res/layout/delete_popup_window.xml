<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/delete_pop_bg"
>

    <Button android:id="@+id/delete_confirm"
        style="@style/MultiSelectActionButton"
        android:text="@string/delete"
        android:background="@drawable/folder_btn_delete"
    />
    <Button android:id="@+id/delete_cancel"
        style="@style/MultiSelectActionButton"
        android:text="@android:string/cancel"
        android:background="@drawable/bottom_btn_bg"
    />
</LinearLayout>
